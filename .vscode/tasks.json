{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "xslt",
			"label": "Transform input.xml",
			"saxonJar": "Saxon-HE-10.0.jar",
			"xsltFile": "transform.xsl",
			"xmlSource": "input.xml",
			"resultPath": "output.html",
			"allowSyntaxExtensions40": "off",
			"messageEscaping": "adaptive",
			"group": {
				"kind": "build"
			},
			"problemMatcher": [
				"$saxon-xslt"
			]
		}
	]
}